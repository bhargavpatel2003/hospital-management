<odoo>
  
  <record id="view_delevery_wizard_form" model="ir.ui.view">
    <field name="name">delivery.wizard.form</field>
    <field name="model">delivery.wizard</field>
    <field name="arch" type="xml">
      <form string="delevery Wizard">
        <group>
          <field name="partner_id"/>
        </group>
        <notebook>
          <page string="operation">
            <field name="line_ids">
              <tree editable="bottom">
                <field name="product_id"/>
                <field name="date"/>
                <field name="deadline_date"/>
                <field name="demand_qty"/>
                <field name="reserve_qty"/>
                <field name="done_qty"/>
              </tree>
            </field>
          </page>
        </notebook>
        <!-- <footer>
          <button name="action_confirm_invoice" type="object" string="Create Invoice" class="btn-primary"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer> -->
      </form>
    </field>
  </record>

  <record id="action_delevery_wizard" model="ir.actions.act_window">
    <field name="name">Delevery</field>
    <field name="res_model">delivery.wizard</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
  </record>

  
</odoo>
